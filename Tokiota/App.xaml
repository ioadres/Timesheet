<?xml version="1.0" encoding="utf-8"?>
<prism:PrismApplication xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    xmlns:prism="clr-namespace:Prism.Unity;assembly=Prism.Unity.Forms" x:Class="Tokiota.App">
    <prism:PrismApplication.Resources>
        <ResourceDictionary>
            <!-- BackgroundColor -->
            <Color x:Key="PrimaryBackgroundColor">#AA0001</Color>
            <Color x:Key="SecundaryBackgroundColor">#272c33</Color>
            <Color x:Key="TertiaryBackgroundColor">#0E3955</Color>
            <Color x:Key="QuinaryBackgroundColor">#003455</Color>
            <Color x:Key="SenaryBackgroundColor">#0C9DF6</Color>
            <!--General Colors-->
            <Color x:Key="FontColor">#52656b</Color>
            <Color x:Key="FontMenuColor">#ffffff</Color>
            <Color x:Key="FontMenuItemColor">#ffffff</Color>
            <Color x:Key="AccentColor1">#cdff00</Color>
            <Color x:Key="AccentColor2">#b8b89f</Color>
            <Color x:Key="BackgroundColor">#f9f9f9</Color>
            <!--Fonts-->
            <OnPlatform x:Key="GeneralButtonFont" x:TypeArguments="Font" iOS="12" Android="14" WinPhone="16" />
            <!--Styles-->
        </ResourceDictionary>
        <!-- Application resource dictionary -->
    </prism:PrismApplication.Resources>
</prism:PrismApplication>